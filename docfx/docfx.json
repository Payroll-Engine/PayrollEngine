{
  "metadata": [
    {
      "src": [
        {
          "files": [ "/**.csproj" ],
          "src": "../../PayrollEngine.Core/Core/"
        },
        {
          "files": [ "/**.csproj" ],
          "src": "../../PayrollEngine.Client.Core/Client.Core/"
        },
        {
          "files": [ "/**.csproj" ],
          "src": "../../PayrollEngine.Client.Scripting/Client.Scripting/"
        },
        {
          "files": [ "/**.csproj" ],
          "src": "../../PayrollEngine.Client.Test/Client.Test/"
        }
      ],
      "dest": "api",
      "includePrivateMembers": false,
      "disableGitFeatures": false,
      "disableDefaultFilter": false,
      "noRestore": false,
      "namespaceLayout": "nested",
      "memberLayout": "samePage",
      "allowCompilationErrors": false,
      "properties": {
        "TargetFramework": "net7.0"
      }
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/**.yml",
          "api/index.md"
        ]
      },
      {
        "files": [
          "articles/**.md",
          "articles/**/toc.yml",
          "toc.yml",
          "*.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "output": "_site",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default",
      "modern"
    ],
    "postProcessors": [],
    "keepFileLink": false,
    "disableGitFeatures": false
  }
}