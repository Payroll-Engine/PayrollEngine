              "wageTypeNumber": 101,
              "name": "Monatslohn",
              "valueExpression": "var values = GetCaseValues(\"Monatslohn\", \"Beschäftigungsgrad\"); return values[\"Monatslohn\"] * values[\"Beschäftigungsgrad\"];",
              "collectors": [
                "AHV Basis",
