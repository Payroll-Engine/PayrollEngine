              "created": "2019-01-01T00:00:00.0Z"
            },
            {
              "wageTypeNumber": 101.2,
              "name": "Monatslohn.Debit",
              "valueExpression": "CaseValue[\"Monatslohn\"] * -0.05M",
              "collectors": [
                "Debit"
              ],
              "created": "2019-01-01T00:00:00.0Z"
            },
            {
              "wageTypeNumber": 102,
              "name": "Stundenlohn",
              "valueExpression": "var values = GetCaseValues(\"Anzahl Stunden\", \"Stundenlohn\"); return values[\"Anzahl Stunden\"] * values[\"Stundenlohn\"];",
              "collectors": [
                "AHV Basis",
                "QST Basis"
